page {
  color: #323233;
  font-size: 28rpx;
}

.demo-logo {
  width: 80vw;
  margin-left: 14vw;
  display: flex;
  margin-bottom: 60rpx;
}

.logo-right {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  margin-left: 30rpx;
}

.demo-home {
margin: 0 auto;
}

.demo-home-nav__group {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.demo-home-nav__title {
  width: 80vw;
  margin: 24rpx 0 8rpx 10vw;
  color: rgba(69, 90, 100, 0.6);
  font-size: 22rpx;
}

.item-nav{
  background: #eeeeee;
  width: 80vw;
  height: 75rpx;
  line-height: 75rpx;
  margin: 10rpx 0;
  border-radius: 45rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
  font-weight: 600;
  display: flex;
  justify-content: space-between;
  align-items: center;
}